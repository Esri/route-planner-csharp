        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Vehicle and driver name combo box item template -->

    <converters:ObjectWithSpecialtiesComboBoxNameConverter x:Key="objectWithSpecialtiesComboBoxNameConverter"/>

    <DataTemplate x:Key="ObjectWithSpecialtiesNameInComboBoxTemplate">
        <app_controls:BindableTextBlock BindableInlines="{Binding Converter={StaticResource objectWithSpecialtiesComboBoxNameConverter}}" />
    </DataTemplate>

    <!--- Vehicle Editor -->
    
    <DataTemplate x:Key="VehicleCell">
        <TextBlock Text="{Binding}"/>
    </DataTemplate>

    <xcdg:CellEditor x:Key="VehicleEditor">
        <xcdg:CellEditor.EditTemplate>

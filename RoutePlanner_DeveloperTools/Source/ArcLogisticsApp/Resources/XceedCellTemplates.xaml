    <!--End of address field cell editors-->

    <!-- Type cell editor -->

    <ObjectDataProvider MethodName="GetValues"
                        ObjectType="{x:Type sys:Enum}"
                        x:Key="TypeValues">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="arcLogistics:OrderType" />
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>

    <converters:OrderTypeConverter x:Key="orderTypeConverter" />

    <DataTemplate x:Key="TypeCellTemplate">
        <TextBlock Text="{Binding Converter={StaticResource orderTypeConverter}}"/>
    </DataTemplate>

    <xcdg:CellEditor x:Key="TypeEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:ComboBoxBasedEditor Height="{StaticResource XceedCustomEditorDefaultHeight}" 
                    VerticalAlignment="Stretch"
                    Converter="{StaticResource orderTypeConverter}"
                    AllItems="{Binding Source={StaticResource TypeValues}}"
                    SelectedElement="{xcdg:CellEditorBinding}">
                </app_controls:ComboBoxBasedEditor>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Priority cell editor -->

    <converters:OrderPriorityConverter x:Key="orderPriorityConverter" />
    
    <ObjectDataProvider MethodName="GetValues"
                        ObjectType="{x:Type sys:Enum}"
                        x:Key="PriorityValues">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="arcLogistics:OrderPriority" />
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>

    <DataTemplate x:Key="PriorityCellTemplate">
        <TextBlock Text="{Binding Converter={StaticResource orderPriorityConverter}}"/>
    </DataTemplate>

    <xcdg:CellEditor x:Key="PriorityCellEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:ComboBoxBasedEditor 
                    Height="{StaticResource XceedCustomEditorDefaultHeight}" 
                    Converter="{StaticResource orderPriorityConverter}"
                    AllItems="{Binding Source={StaticResource PriorityValues}}"
                    SelectedElement="{xcdg:CellEditorBinding}">
                </app_controls:ComboBoxBasedEditor>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Days cell editor -->

    <xcdg:CellEditor x:Key="DaysCellEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:DaysEditor 
                    Height="{StaticResource XceedCustomEditorDefaultHeight }"
                    Days="{Binding}"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>
    
    <!-- Route insertion row Name cell editor -->

    <xcdg:CellEditor x:Key="InsertionRowRouteNameEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:RouteNameSelector 
                    Height="{StaticResource XceedCustomEditorDefaultHeight }"
                    NewRoute="{xcdg:CellEditorBinding}" 
                    DefaultRoutes="{Binding Source={x:Static Application.Current}, Path=Project.DefaultRoutes}"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>


    <!-- Default check box editor -->


    <DataTemplate x:Key="DefaultCheckBoxTemplate">
        <Grid Background="Transparent"
              FocusVisualStyle="{x:Null}"
              VerticalAlignment="Stretch" 
              HorizontalAlignment="Stretch">
            <CheckBox VerticalAlignment="Center"
                  HorizontalAlignment="Center"
                  Focusable="False"
                  Cursor="Arrow"
                  IsChecked="{Binding Mode=OneWay}"
                  FocusVisualStyle="{x:Null}"
                  />
            <Grid Background="Transparent" 
                  FocusVisualStyle="{x:Null}"
                  VerticalAlignment="Stretch"
                  HorizontalAlignment="Stretch"
                  Width="Auto"
                  Height="Auto"
                  />
        </Grid>
    </DataTemplate>

    <xcdg:CellEditor x:Key="DefaultCheckBoxEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <Grid Background="Transparent" 
                      Margin="0"
                      VerticalAlignment="Stretch" 
                      HorizontalAlignment="Stretch">
                    <CheckBox VerticalAlignment="Center"
                  HorizontalAlignment="Center"
                              Cursor="Hand"
                              Width="Auto"
                              IsChecked="{xcdg:CellEditorBinding}"
                  />
                </Grid>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Default check box editor -->


    <!-- Checkbox editor for TimeWindow's day. -->

    <converters:DayToBooleanConverter x:Key="dayToBooleanConverter"/>
    <converters:DataContextToVisibilityConverter x:Key="dataContextToVisibilityConverter"/>

    <DataTemplate x:Key="DayCheckBoxTemplate">
        <Grid Background="Transparent"
            FocusVisualStyle="{x:Null}"
            VerticalAlignment="Stretch" 
            HorizontalAlignment="Stretch">
            <CheckBox VerticalAlignment="Center"
                HorizontalAlignment="Center"
                Focusable="False"
                Cursor="Arrow"
                Visibility="{Binding Converter={StaticResource dataContextToVisibilityConverter}}"
                IsChecked="{Binding Mode=OneWay,Converter={StaticResource dayToBooleanConverter}}"
                FocusVisualStyle="{x:Null}"
                />
            <Grid Background="Transparent" 
                FocusVisualStyle="{x:Null}"
                VerticalAlignment="Stretch"
                HorizontalAlignment="Stretch"
                Width="Auto"
                Height="Auto"
                />
        </Grid>
    </DataTemplate>

    <xcdg:CellEditor x:Key="DayCheckBoxEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <Grid Background="Transparent" 
                    Margin="0"
                    VerticalAlignment="Stretch" 
                    HorizontalAlignment="Stretch">
                    <CheckBox VerticalAlignment="Center"
                        Visibility="{Binding Converter={StaticResource dataContextToVisibilityConverter}}"
                        HorizontalAlignment="Center"
                        Cursor="Hand"
                        Width="Auto"
                        IsChecked="{xcdg:CellEditorBinding Converter={StaticResource dayToBooleanConverter}}"
                        />
                </Grid>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Symbol box editor -->

    <converters:SymbolConverter x:Key="symbolConverter"/>
    <converters:SymbolsConverter x:Key="symbolsConverter"/>
    
    <DataTemplate x:Key="SymbolCell">
        <ContentPresenter Height="{StaticResource XceedCustomEditorDefaultHeight}" Content="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type xcdg:Cell}},
                                            Path=DataContext,
                                            Converter={StaticResource symbolConverter}}"/>
    </DataTemplate>

    <xcdg:CellEditor x:Key="SymbolEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <Grid>
                    <ComboBox Height="{StaticResource XceedCustomEditorDefaultHeight}"
                                       HorizontalContentAlignment="Center"
                         ItemsSource="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type xcdg:Row}},
                                     Path=DataContext,Converter={StaticResource symbolsConverter}}"
                        SelectedItem="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type xcdg:Row}},
                                     Path=DataContext,Converter={StaticResource symbolConverter}}"/>
                    <ContentControl Height="{StaticResource XceedCustomEditorDefaultHeight}" Content="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type xcdg:Cell}},
                                                Path=DataContext,
                                                Converter={StaticResource symbolConverter}}"/>
                </Grid>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <converters:QuantityConverter x:Key="quantityConverter"/>

    <DataTemplate x:Key="QuantityMinCell">
        <TextBlock Text="{Binding RelativeSource={RelativeSource FindAncestor, 
                                                 AncestorType={x:Type xcdg:Cell}},
                                 Path=DataContext, 
                                 Converter={StaticResource quantityConverter}, 
                                 ConverterParameter='MinValue'}"/>
    </DataTemplate>
    
    <DataTemplate x:Key="QuantityMaxCell">
        <TextBlock Text="{Binding RelativeSource={RelativeSource FindAncestor, 
                                                 AncestorType={x:Type xcdg:Cell}},
                                 Path=DataContext, 
                                 Converter={StaticResource quantityConverter}, 
                                 ConverterParameter='MaxValue'}"/>
    </DataTemplate>

     <!-- Is Locked Icon -->

    <DataTemplate 
        x:Key="IsLockedIcon">
        <Grid Background="{DynamicResource IsLockedGridColumnBrush}" 
              Width="14"
              Height="14"
              ToolTip="Locked"
              Margin="1,1,0,0"
              VerticalAlignment="Stretch"
              HorizontalAlignment="Stretch"/>
    </DataTemplate>


    <!-- Locked Check box template -->


    <DataTemplate x:Key="LockedCheckBoxTemplate">
        <Grid  Background="Transparent"
              FocusVisualStyle="{x:Null}"
              VerticalAlignment="Stretch" 
              HorizontalAlignment="Stretch">
            <CheckBox VerticalAlignment="Stretch"
                      Height="Auto" Width="Auto"
                      HorizontalAlignment="Stretch"
                      Style="{DynamicResource LockedCheckBoxStyle}"
                      Focusable="False"
                  Cursor="Arrow"
                  IsChecked="{Binding Mode=OneWay}"
                  FocusVisualStyle="{x:Null}"
                  />
            <Grid Background="Transparent" 
                  FocusVisualStyle="{x:Null}"
                  VerticalAlignment="Stretch"
                  HorizontalAlignment="Stretch"
                  Width="Auto"
                  Height="Auto"
                  />
        </Grid>
    </DataTemplate>

    <xcdg:CellEditor x:Key="LockedCheckBoxEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <CheckBox VerticalAlignment="Stretch" Margin="-1"
                          Height="Auto" Width="Auto"
                  HorizontalAlignment="Stretch"
                  Style="{DynamicResource LockedCheckBoxStyle}"
                              IsChecked="{xcdg:CellEditorBinding}"    
                  />
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>


    <!-- StopType Content template -->
    
    <converters:StopTypeConverter x:Key="stopTypeConverter"/>
    
    <DataTemplate x:Key="StopTypeCellTemplate">
        <Grid Background="{x:Null}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
            <ContentControl Height="{StaticResource XceedCustomEditorDefaultHeight}">
                <ContentControl.Content>  
                    <!-- Multi binding used for pass Stop as parameter to converter and handle event about Status property changed. -->
                    <MultiBinding Converter="{StaticResource stopTypeConverter}">
                        <Binding Path="."/>
                        <Binding Path=".IsViolated"/>
                        <Binding Path=".Route.Color"/>
                    </MultiBinding>
                </ContentControl.Content>
            </ContentControl>
        </Grid>
    </DataTemplate>

    <!-- Sequence number content template -->

    <converters:SequenceNumberConverter x:Key="sequenceNumberConverter"/>
    
    <DataTemplate x:Key="SequenceNumberTemplate">
        <TextBlock Text="{Binding Converter={StaticResource sequenceNumberConverter}}" />
    </DataTemplate>

    <!-- Visibility Icon -->
    
    <DataTemplate x:Key="VisibilityIcon">
        <Grid Width="14" 
              Height="14" 
              Margin="1,1,0,0"
              HorizontalAlignment="Center" 
              VerticalAlignment="Center"
              Background="{DynamicResource IsVisibleGridColumnBrush}"
              ToolTip="Visible"/>
    </DataTemplate>

    <!-- Visibility Check box template -->

    <DataTemplate x:Key="VisibilityCheckBoxTemplate">
        <Grid  Background="Transparent"
              FocusVisualStyle="{x:Null}"
              VerticalAlignment="Stretch" 
              HorizontalAlignment="Stretch">
            <CheckBox VerticalAlignment="Stretch"
                      Height="{StaticResource XceedCustomEditorDefaultHeight}" Width="20"
                  Style="{DynamicResource VisibilityCheckBoxStyle}"
                  HorizontalAlignment="Center"
                  Focusable="False"
                  Cursor="Arrow"
                  IsChecked="{Binding Mode=OneWay}"
                  FocusVisualStyle="{x:Null}"
                  />
            <Grid Background="Transparent" 
                  FocusVisualStyle="{x:Null}"
                  VerticalAlignment="Stretch"
                  HorizontalAlignment="Stretch"
                  Width="Auto"
                  Height="Auto"
                  />
        </Grid>
    </DataTemplate>
    <xcdg:CellEditor x:Key="VisibilityCheckBoxEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <CheckBox VerticalAlignment="Stretch"
                          Height="Auto"
                          Width="Auto"
                          HorizontalAlignment="Stretch"
                          Margin="-1"
                          Style="{DynamicResource VisibilityCheckBoxStyle}"
                          IsChecked="{xcdg:CellEditorBinding}" />
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Restriction Name cell template -->

    <converters:RestrictionNameConverter x:Key="restrictionNameConverter"/>

    <DataTemplate x:Key="restrictionNameContentTemplate">
        <app_controls:BindableTextBlock BindableInlines="{Binding Converter={StaticResource restrictionNameConverter}}" TextWrapping="Wrap" />
    </DataTemplate>

    <!-- Restriction Parameter cell template -->

    <!-- Template for non restriction usage parameter.-->
    <converters:RestrictionParameterValueConverter x:Key="restrictionParameterValueConverter"/>
    <converters:RestrictionParameterPresentConverter x:Key="restrictionParameterPresentConverter"/>
    <DataTemplate x:Key="restrictionParameterContentTemplate">
        <app_controls:BindableTextBlock 
            Text="{Binding Converter={StaticResource restrictionParameterValueConverter}}"
            IsEnabled="{Binding Converter={StaticResource restrictionParameterPresentConverter}}"
            TextWrapping="Wrap" />
    </DataTemplate>

    <converters:RestrictionUsageLabelConverter x:Key="restrictionPreferenceValueConverter"/>
    <DataTemplate x:Key="restrictionUsageContentTemplate">
        <TextBlock Text="{Binding Converter={StaticResource restrictionPreferenceValueConverter}}"/>
    </DataTemplate>
    
    <converters:RestrictionPreferenceAllValuesConverter x:Key="restrictionPreferenceAllValuesConverter"/>
    <converters:RestrictionPreferenceSelectedValueConverter x:Key="restrictionPreferenceSelectedValueConverter"/>
    <xcdg:CellEditor x:Key="restrictionUsageEditTemplate">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate >
                <ComboBox
                    ItemsSource="{Binding 
                        Converter={StaticResource restrictionPreferenceAllValuesConverter}}" 
                    SelectedItem="{xcdg:CellEditorBinding 
                        Converter={StaticResource restrictionPreferenceSelectedValueConverter} }"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>
    
    <!-- Report Name cell template -->

    <converters:ReportNameConverter x:Key="reportNameConverter"/>

    <DataTemplate x:Key="reportNameContentTemplate">
        <app_controls:BindableTextBlock BindableInlines="{Binding Converter={StaticResource reportNameConverter}}" TextWrapping="Wrap" />
    </DataTemplate>


    <!-- Route Name cell template -->

    <converters:RouteNameConverter x:Key="routeNameConverter"/>

    <DataTemplate x:Key="routeNameContentTemplate">
        <app_controls:BindableTextBlock BindableInlines="{Binding Converter={StaticResource routeNameConverter}}" />
    </DataTemplate>
    
    <xcdg:CellEditor x:Key="RouteNameEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <TextBox Text="{Binding Path=Name}" />
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Date time cell editor -->

    <xcdg:CellEditor x:Key="DateTimeEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:DatePickerEx
                    Height="{StaticResource XceedCustomEditorDefaultHeight}"
                    SelectedDate="{xcdg:CellEditorBinding}"
                    VerticalAlignment="Stretch"
                    HorizontalAlignment="Stretch"
                    BorderThickness="0"
                    Style="{DynamicResource DefaultDatePickerStyle}"
                    CalendarStyle="{DynamicResource calendarStyle}"
                    />
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <converters:DateFormatConverter x:Key="dateFormatConverter"/>
    
    <DataTemplate x:Key="DateTimeTemplate">
        <TextBlock Text="{Binding Converter={StaticResource dateFormatConverter}}"/>
    </DataTemplate>
    
    <!-- ///// Xceed custom comparers ///// -->
    
    <grid_helpers:ToStringComparer x:Key="toStringComparer"/>
    <grid_helpers:TimeWindowComparer x:Key="timeWindowComparer"/>
    <grid_helpers:BreakComparer x:Key="breakComparer"/>
    <grid_helpers:ColorComparer x:Key="colorComparer"/>
    <grid_helpers:BreaksComparer x:Key="breaksComparer"/>

    <xcdg:PrintTableView x:Key="printView" 
                         HorizontalGridLineThickness="1"
                         HorizontalGridLineBrush="{StaticResource gridPrintBorderBrush}">
        <xcdg:PrintTableView.PageFooters>
            <DataTemplate>
                <TextBlock Text="{xcdg:ViewBinding CurrentPageNumber}"
                          HorizontalAlignment="Center"
                          FontFamily="{StaticResource DefaultApplicationFont}"
                          FontWeight="Bold"
                          FontSize="{StaticResource MiddleFontSize}"/>
            </DataTemplate>
        </xcdg:PrintTableView.PageFooters>
    </xcdg:PrintTableView>

</ResourceDictionary>

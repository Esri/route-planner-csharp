    </DataTemplate>

    <xcdg:CellEditor x:Key="UnitEditorTemplate">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:UnitTextBoxEditor Height="{StaticResource XceedCustomEditorDefaultHeight}"

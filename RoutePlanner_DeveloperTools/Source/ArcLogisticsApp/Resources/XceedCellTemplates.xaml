    </xcdg:CellEditor>

    <!-- DataObject Name Editor -->

    <converters:DataObjectToNameConverter x:Key="dataObjectToNameConverter"/>

    <xcdg:CellEditor x:Key="DataObjectNameEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <xceed_controls:AutoSelectTextBox
                    Margin="-2,-1,0,1"
                    BorderThickness="0"
                    Background="Transparent"
                    AutoSelectBehavior="Never"
                    AutoMoveFocus="True"
                    BorderBrush="Transparent"
                    AcceptsReturn="False"
                    Height="{StaticResource XceedCustomEditorDefaultHeight}"
                    Text="{Binding Path=DataContext,
                           RelativeSource={RelativeSource TemplatedParent},
                           Converter={StaticResource dataObjectToNameConverter}}">
                    <xceed_controls:AutoSelectTextBox.ContextMenu>
                        <ContextMenu Name="menu" Height="0" Width="0"/>
                    </xceed_controls:AutoSelectTextBox.ContextMenu>
                </xceed_controls:AutoSelectTextBox>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>


    <converters:SyncTypeConverter x:Key="syncTypeConverter" />
    
    <!-- Sync type data -->
    
    <ObjectDataProvider MethodName="GetValues"
                        ObjectType="{x:Type sys:Enum}"
                        x:Key="SyncTypeValues">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="arcLogistics:SyncType" />
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>

    <!-- Sync type template -->
    
    <DataTemplate x:Key="SyncTypeCell">
        <TextBlock Text="{Binding Converter={StaticResource syncTypeConverter}}"/>
    </DataTemplate>
    
    <!-- Sync Type editor -->
    
    <xcdg:CellEditor x:Key="SyncTypeEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:ComboBoxBasedEditor
                    Height="{StaticResource XceedCustomEditorDefaultHeight}" 
                    AllItems="{Binding Source={StaticResource SyncTypeValues}}"
                    SelectedElement="{xcdg:CellEditorBinding}"
                    Converter="{StaticResource syncTypeConverter}">
                </app_controls:ComboBoxBasedEditor>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Mobile Devices editor -->

    <DataTemplate x:Key="MobileDevicesCell">

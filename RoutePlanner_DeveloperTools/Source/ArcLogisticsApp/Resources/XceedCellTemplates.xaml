        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Devices editor -->
    
    <DataTemplate x:Key="DevicesCell">
        <TextBlock Text="{Binding}"/>
    </DataTemplate>

    <xcdg:CellEditor x:Key="DevicesEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:ComboBoxBasedEditor
                    Height="{StaticResource XceedCustomEditorDefaultHeight }"
                    AllItems="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type pages:PageBase}}, Path=Devices}"
                    SelectedItem="{xcdg:CellEditorBinding}"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Folder editor -->

    <DataTemplate x:Key="FolderCell">
        <TextBlock Text="{Binding}"/>
    </DataTemplate>

    <xcdg:CellEditor x:Key="FolderEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:FolderEditor Folder="{xcdg:CellEditorBinding}"
                                           Height="{StaticResource XceedCustomEditorDefaultHeight }"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Arrive Time editor -->

    <converters:ArriveTimeConverter x:Key="arriveTimeConverter" />

    <DataTemplate x:Key="arriveTimeCellContentTemplate">
        <TextBlock Text="{Binding Converter={StaticResource arriveTimeConverter}}" />
    </DataTemplate>


    <converters:BarrierEffectConverter x:Key="barrierEffectConverter"/>

    <DataTemplate x:Key="BarrierEffectTemplate">
        <TextBlock>
            <TextBlock.Text>
                <!-- Multi binding used for pass Text as parameter and control which shows this text. -->
                <MultiBinding Converter="{StaticResource barrierEffectConverter}">
                    <Binding RelativeSource="{RelativeSource FindAncestor, AncestorType={x:Type xcdg:Row}}" Path="DataContext"/>
                    <Binding RelativeSource="{RelativeSource FindAncestor, AncestorType={x:Type xcdg:Row}}" Path="DataContext.Geometry"/>
                    <Binding Path="DelayTime"/>
                    <Binding Path="BlockTravel"/>
                    <Binding Path="SpeedFactorInPercent"/>
                </MultiBinding>
            </TextBlock.Text>
        </TextBlock>
    </DataTemplate>

    <xcdg:CellEditor x:Key="BarrierEffectCellEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:BarrierCellEditor 
                    Height="{StaticResource XceedCustomEditorDefaultHeight }"
                    BarrierEffect="{Binding}"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

 <!-- //////////////////////////////////////////////////////////////////////////////////////// -->

    <converters:AddLocationConverter x:Key="addLocationConverter"/>

    <DataTemplate x:Key="AddLocationAddressLineContent">
        <TextBlock>
            <TextBlock.Text>
                <!-- Multi binding used for pass Text as parameter and control which shows this text. -->
                <MultiBinding Converter="{StaticResource addLocationConverter}">
                    <Binding Path="."/>
                    <Binding RelativeSource="{RelativeSource Self}" Path="."/>
                </MultiBinding>
            </TextBlock.Text>
        </TextBlock>
    </DataTemplate>

    <!--Keep intact keys for the following address field cell editors-->
    
    <xcdg:CellEditor x:Key="FullAddressEditor">

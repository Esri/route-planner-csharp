<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:ESRI.ArcLogistics.App"
    xmlns:converters="clr-namespace:ESRI.ArcLogistics.App.Converters"
    xmlns:arcLogistics="clr-namespace:ESRI.ArcLogistics;assembly=ESRI.ArcLogisticsNG"
    xmlns:domain_objects="clr-namespace:ESRI.ArcLogistics.DomainObjects;assembly=ESRI.ArcLogisticsNG"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:xcdg="http://schemas.xceed.com/wpf/xaml/datagrid"
    xmlns:xceed_controls="clr-namespace:Xceed.Wpf.Controls;assembly=Xceed.Wpf.Controls"
    xmlns:app="clr-namespace:ESRI.ArcLogistics.App"
    mc:Ignorable="d"
    xmlns:app_controls="clr-namespace:ESRI.ArcLogistics.App.Controls"
    xmlns:controls="clr-namespace:System.Windows.Controls;assembly=PresentationFramework"
    xmlns:pages="clr-namespace:ESRI.ArcLogistics.App.Pages"
    xmlns:grid_helpers="clr-namespace:ESRI.ArcLogistics.App.GridHelpers"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">


    <!-- //////////////////////////////////////////////////////////////////////////////////////// -->

    <!-- Default string editor -->
    <xcdg:CellEditor x:Key="DefaultStringEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <xceed_controls:AutoSelectTextBox
                    Margin="-2,-1,0,1"
                    BorderThickness="0"
                    Text="{xcdg:CellEditorBinding}"
                    Background="Transparent"
                    AutoSelectBehavior="Never"
                    AutoMoveFocus="True"
                    BorderBrush="Transparent" AcceptsReturn="False">
                    <xceed_controls:AutoSelectTextBox.ContextMenu>
                        <ContextMenu Name="menu" Height="0" Width="0"/>
                    </xceed_controls:AutoSelectTextBox.ContextMenu>
                </xceed_controls:AutoSelectTextBox>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <DataTemplate x:Key="DefaultStringTemplate">
        <TextBlock Text="{xcdg:CellEditorBinding}" Margin="1,0,0,0"/>
    </DataTemplate>

    <!-- Custom order property editors -->
   <xcdg:CellEditor x:Key="CustomOrderPropertyTextEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:CustomOrderPropertyTextBox
                    FontSize="{StaticResource MiddleFontSize}"
                    Margin="-2,-1,0,1"
                    BorderThickness="0"
                    Text="{xcdg:CellEditorBinding}"
                    Background="Transparent"
                    AutoSelectBehavior="Never"
                    AutoMoveFocus="False"
                    BorderBrush="Transparent"
                    AcceptsReturn="False" />
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
   </xcdg:CellEditor>

   <xcdg:CellEditor x:Key="CustomOrderPropertyNumericEditor">
      <xcdg:CellEditor.EditTemplate>
         <DataTemplate>
            <app_controls:CustomOrderPropertyNumericTextBox
               NumberStyles="Float"
               NullValue="0"
               FontSize="{StaticResource MiddleFontSize}"
               Margin="-2,-1,0,1"
               BorderThickness="0"
               Value="{xcdg:CellEditorBinding}"
               Background="Transparent"
               AutoSelectBehavior="Never"
               AutoMoveFocus="False"
               BorderBrush="Transparent"
               AcceptsReturn="False" />
         </DataTemplate>
      </xcdg:CellEditor.EditTemplate>
   </xcdg:CellEditor>

   <!-- MessageWindow Editor -->
    <converters:MessageWindowImageConverter x:Key="messageWindowImageConverter"/>

    <DataTemplate x:Key="MessageWindowCellTemplate">
        <ContentPresenter SnapsToDevicePixels="True"
            VerticalAlignment="Center"
            HorizontalAlignment="Center"
            Height="16" Width="17"
            Margin="1,1,0,0"
            Content="{Binding Converter={StaticResource messageWindowImageConverter}}" />
    </DataTemplate>

    <converters:MessageWindowTextConverter x:Key="messageWindowTextConverter"/>

    <DataTemplate x:Key="MessageWindowTextCellTemplate">
        <app_controls:BindableTextBlock TextWrapping="Wrap" BindableInlines="{Binding Converter={StaticResource messageWindowTextConverter}}" />
    </DataTemplate>

    <!-- Color cell editor -->
    <converters:ColorConverter x:Key="colorConverter"/>

    <DataTemplate x:Key="ColorCell">
        <ContentControl Content="{Binding Converter={StaticResource colorConverter}}"/>
    </DataTemplate>

    <xcdg:CellEditor x:Key="ColorCellEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:GridColorPicker Height="{StaticResource XceedCustomEditorDefaultHeight}"
                                               SelectedColor="{xcdg:CellEditorBinding}"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Fuel type editor -->
    
    <DataTemplate x:Key="FuelTypeCell">
        <TextBlock Text="{Binding}"/>
    </DataTemplate>

    <xcdg:CellEditor x:Key="FuelTypeEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:ComboBoxBasedEditor 
                    Height="{StaticResource XceedCustomEditorDefaultHeight}"
                    AllItems="{Binding Source={x:Static Application.Current}, Path=Project.FuelTypes}" 
                    SelectedItem="{xcdg:CellEditorBinding}"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- TimeWindow Editor -->

    <DataTemplate x:Key="TimeWindowCell">
        <TextBlock Text="{Binding}"/>
    </DataTemplate>

    <xcdg:CellEditor x:Key="TimeWindowEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:CellTimeWindowEditor WideopenVisibility="Visible"  Height="{StaticResource XceedCustomEditorDefaultHeight }"
                                                       TimeWindow="{xcdg:CellEditorBinding}"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <xcdg:CellEditor x:Key="RouteTimeWindowEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:CellTimeWindowEditor WideopenVisibility="Collapsed" Height="{StaticResource XceedCustomEditorDefaultHeight}"
                                                       TimeWindow="{xcdg:CellEditorBinding}"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- TimeSpan Editor -->
    <converters:TimespanConverter x:Key="timeSpanConverter"/>

    <DataTemplate x:Key="TimeSpanCell">
        <TextBlock Text="{Binding Converter={StaticResource timeSpanConverter}}" />
    </DataTemplate>

    <xcdg:CellEditor x:Key="TimeSpanEditor">
        <xcdg:CellEditor.EditTemplate>
            <DataTemplate>
                <app_controls:TimeTextBox HorizontalAlignment="Right" TimeSpan="{xcdg:CellEditorBinding}"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

    <!-- Break Editor -->
    
    <DataTemplate x:Key="BreakCell">
        <TextBlock Text="{Binding}"/>
    </DataTemplate>

    <xcdg:CellEditor x:Key="BreakEditor">
        <xcdg:CellEditor.EditTemplate>

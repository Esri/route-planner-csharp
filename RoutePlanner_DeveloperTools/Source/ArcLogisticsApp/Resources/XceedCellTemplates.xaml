                                                Margin="0"/>
            </DataTemplate>
        </xcdg:CellEditor.EditTemplate>
    </xcdg:CellEditor>

   <!-- Zones geometry template -->

    <converters:GeometryConverter x:Key="geometryConverter"/>

    <DataTemplate x:Key="geometryCellContentTemplate">
        <TextBlock Text="{Binding Converter={StaticResource geometryConverter}, Mode=OneWay}" />
    </DataTemplate>
    
    <!-- Percent editor -->

    <converters:PercentConverter x:Key="percentConverter" />
    
    <DataTemplate x:Key="percentCellContentTemplate">
        <TextBlock Text="{Binding Converter={StaticResource percentConverter}, Mode=OneWay}" />
    </DataTemplate>

